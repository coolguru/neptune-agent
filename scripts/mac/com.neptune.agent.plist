<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.neptune.agent</string>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <true/>
        <key>NetworkState</key>
        <true/>
        <key>WorkingDirectory</key>
        <string>WORKING_DIRECTORY_HERE</string>
        <key>Program</key>
        <string>AGENT_PATH_HERE</string>
        <key>UserName</key>
        <string>AGENT_USER_HERE</string>
        <key>ExitTimeOut</key>
        <integer>2</integer>
    </dict>
</plist>
